!cerrar todos los puertos

enable
configure terminal
interface range fa0/1-24
shutdown
exit
interface range gi0/1-2
shutdown
exit

hostname SW1_CUR
no ip domain-lookup
enable secret cisco

vlan 108
name ventas
exit

vlan 110
name gerencia
exit

vlan 1
name nativa
exit

line con 0
password cisco
login
exit

line vty 0 4
password cisco
login
exit

interface range fa0/2, fa0/3
switchport mode access
no shutdown
exit

!asignar vlans a puertos

!fa02 a gerencia
interface fa0/2
switchport access vlan 110
exit

!fa03 a ventas
interface fa0/3
switchport access vlan 108
exit

interface range g0/1
switchport mode trunk
switchport trunk native vlan 1
no shutdown
exit
exit

copy running-config startup-config 